{
  "mcpServers": {
    "firebase": {
      "command": "npx",
      "args": [
        "-y",
        "firebase-tools@latest",
        "experimental:mcp",
        "--only",
        "auth,firestore"
      ]
    },
    "FastMCP": {
      "command": "",
      "args": []
    },
    "chrome dev tools": {
      "command": "gemini",
      "args": [
        "extensions",
        "install",
        "https://github.com/ChromeDevTools/chrome-devtools-mcp"
      ]
    },
    "genkit": {
      "command": "gemini",
      "args": [
        "extensions",
        "install",
        "https://github.com/gemini-cli-extensions/genkit"
      ]
    }
  }
}